algoritmo "semnome"
var
   A, B, C, Delta: Real
inicio
      Escreva ("Digite o valor de A: ")
      Leia (A)
      
      Escreva ("Digite o valor de B: ")
      Leia (B)

      Escreva ("Digite o valor de C: ")
      Leia (C)
      
      Delta <- (B * B) - 4 * A * C
      
      Escreva ("O valor de delta é ", Delta:5:2)

fimalgoritmo